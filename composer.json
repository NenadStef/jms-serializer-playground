{
    "name": "nenadstef/jms-serializer-playground",
    "description": "This project is for jms serializer exploring and testing",
    "version": "0.0.1",
    "require": {
        "php": "^8.3",
        "jms/serializer": "^3.30",
        "symfony/uid": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Jms\\Serializer\\Playground\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jms\\Serializer\\Playground\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
